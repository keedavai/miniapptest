{
    "name": "directorylister/directorylister",
    "description": "The easiest way to expose the contents of any web-accessible folder for browsing and sharing",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-zip": "*",
        "dflydev/dot-access-data": "^3.0",
        "filp/whoops": "^2.15",
        "illuminate/config": "^10.0",
        "illuminate/container": "^10.0",
        "illuminate/filesystem": "^10.0",
        "league/commonmark": "^2.4",
        "league/mime-type-detection": "^1.14",
        "php-di/php-di": "^7.0",
        "phlak/config": "^10.1",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "symfony/console": "^6.3",
        "symfony/finder": "^6.3",
        "symfony/yaml": "^6.3",
        "twig/twig": "^3.7",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "DirectoryLister\\": "app/src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}